<input
  type="hidden"
  name="authenticity_token"
  value="<%= form_authenticity_token %>"
>
<div class="form-group"
  <label for="user_email">
   Email
  </label>
  <input
    class="form-control"
    type="email"
    id="user_email"
    name="user[email]"
    placeholder="email"
  >
</div>

<div class="form-group">
  <label for="user_password">
    Password
  </label>
  <input
    class="form-control"
    type="password"
    id="user_password"
    name="user[password]"
    placeholder="password"
  >
</div>
