<input
  type="hidden"
  name="authenticity_token"
  value="<%= form_autheticity_token %>"
>
<label for="user_username">
  Username
</label>
<input
  type="text"
  id="user_username"
  name="user[username]"
  value="<%= user.username %>"
>
<label for="user_password">
  Password
</label>
<input
  type="password"
  id="user_password"
  name="user[password]"
  value="<%= user.password %>"
>
